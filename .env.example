# Interactive Brokers Connection
IB_HOST=127.0.0.1
IB_PORT=7497
IB_CLIENT_ID=1

# AWS Configuration
AWS_REGION=us-east-1
S3_BUCKET=your-s3-bucket-name
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Database
